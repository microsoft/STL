RUNALL_INCLUDE .\prefix.lst
RUNALL_CROSSLIST
*	PM_CL="/w14640 /Zc:threadSafeInit-"
RUNALL_CROSSLIST
PM_CL="/EHsc /MD /D_ITERATOR_DEBUG_LEVEL=0 /std:c++14"
