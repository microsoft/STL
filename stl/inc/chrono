        _NODISCARD constexpr chrono::weekday weekday() const noexcept;
        _NODISCARD constexpr unsigned index() const noexcept;
        _NODISCARD constexpr bool ok() const noexcept;
    };

    class weekday_last {
        chrono::weekday _Weekday;

    public:
        constexpr explicit weekday_last(const chrono::weekday& _Wd) noexcept;

        _NODISCARD constexpr chrono::weekday weekday() const noexcept;
        _NODISCARD constexpr bool ok() const noexcept;
    };

