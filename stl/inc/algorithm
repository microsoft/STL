    // OR sets [_First1, _Last1) and [_First2, _Last2), array dest
    return _STD set_union(_First1, _Last1, _First2, _Last2, _Dest, less<>());
}
#endif // _ITERATOR_DEBUG_ARRAY_OVERLOADS

#if _HAS_CXX17
