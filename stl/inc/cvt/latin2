            0x00aa, 0x00b6, 0x00c6, 0x00b7, 0x00c3, 0x00ad, 0x00b2, 0x00b3, 0x00d7};

        template <class _Elem, unsigned long _Maxcode = 0xffff>
        class codecvt_latin2 : public _Cvt_one_byte<_Elem, _tab_latin2<int>, _Maxcode> {};
